<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title><%= title %></title>
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="format-detection" content="telephone=no"/>
    <link rel="shortcut icon" href="/favicon.ico">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <script type='text/javascript' src='/public/static/js/lib/jquery-3.1.0.min.js' charset='utf-8'></script>
    <link rel="stylesheet" href="/public/static/css/dhy/module/myCouponWeuiAll.css">
</head>
<body ontouchstart>
<div class="coupon-get-pages">
    <div class="coupon-get-msg">
        <div class="cgm-icon-area"><i class="iconfont icon-gou"></i></div>
        <div class="cgm-text-area">
            <% if(payType=="1"){ %>
                <h2>支付<%= payment %>积分，领取成功</h2>
            <% }else if(payType=="2"){ %>
                <h2>支付&yen;<%= payment %>元，领取成功</h2>
            <% }else{ %>
                <h2>支付领取成功</h2>
            <% } %>

            <% if(code.length>1){ %>
                <p>请及时使用</p>
                <p>(数量:<%= code.length %>,请左右滑动查看)</p>
            <% }else {%>
                <p>请及时使用</p>
            <% } %>
        </div>
    </div>
    
    <% if(code.length>0){ %>
    <div class="swiper-container">
        <!-- Additional required wrapper -->
        <div class="swiper-wrapper">
            <!-- Slides -->
            <% code.forEach(function (item) { %>
            <div class="swiper-slide">

                <div class="coupon-get-ops">
                    <div class="cgo-row"><%= couponName %></div>
                    <div class="cgo-row">
                        <div class="cgo-row-hd">优惠券码</div>
                        <div class="cgo-row-bd"><%= item %></div>
                    </div>
                    <div class="cgo-code-area">
                        <p class="cca-code">
                            <img src="/code/qrcode/<%= item %>?eclevel=M&version=6&scale=4"/>
                        </p>
                        <p class="cca-text">向商家出示以上码券即可消费</p>
                    </div>
                    <p class="cgo-btn-area">
                        <% if(couponType=="1"||couponType=="2"||couponType=="3"||couponType=="7"){ %>
                        <a href="/dhy/myCouponList?channel=<%= channel %>&type=member&status=1" class="weui-btn weui-btn_primary weui-btn_inline">查看我的优惠券</a>
                        <% }if(couponType=="0" || couponType=="4" ||couponType=="5"|| couponType=="6"){%>
                        <a href="/dhy/myCouponList?channel=<%= channel %>&type=siebel&status=可用" class="weui-btn weui-btn_primary weui-btn_inline">查看我的优惠券</a>
                        <% }else if(couponType==undefined) { %>
                        <a href="/#/myCoupons?channel=<%= channel %>" class="weui-btn weui-btn_primary weui-btn_inline">查看我的优惠券</a>
                        <% } %>
                    </p>
                </div>

            </div>
            <% }) %>
        </div>
        <!-- If we need pagination -->
        <div class="swiper-pagination"></div>
    </div>
    <% } %>

</div>

<script type='text/javascript' src='/public/static/js/extends/weui1.0.1/jquery-weui.min.js' charset='utf-8'></script>
<script type='text/javascript' src='/public/static/js/extends/weui1.0.1/swiper.min.js' charset='utf-8'></script>
<script type="text/javascript" src="/public/static/js/extends/fastclick.js"></script>
<script type="text/javascript">
    $(function () {
        FastClick.attach(document.body);

        $(".swiper-container").swiper()
    })
</script>
</body>
</html>